{
  "name": "sea-logistics",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "node server.js",
    "test:email": "tsx test-email.ts",
    "clean": "node -e \"const fs=require('fs');const path=require('path');function rmdir(dir){if(fs.existsSync(dir)){fs.readdirSync(dir).forEach(f=>{const cur=path.join(dir,f);fs.statSync(cur).isDirectory()?rmdir(cur):fs.unlinkSync(cur);});fs.rmdirSync(dir);}}['dist','node_modules/.vite'].forEach(d=>rmdir(d));console.log('✅ Arquivos temporários removidos!')\"",
    "clean:logs": "node -e \"const fs=require('fs');const glob=require('glob');glob.sync('**/*.log').forEach(f=>fs.unlinkSync(f));console.log('✅ Logs removidos!')\""
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@react-oauth/google": "^0.12.2",
    "@types/gapi": "^0.0.47",
    "@types/gapi.auth2": "^0.0.61",
    "@types/gapi.client.gmail": "^1.0.5",
    "@types/nodemailer": "^6.4.17",
    "borderless": "^1.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "firebase": "^10.7.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.303.0",
    "nodemailer": "^6.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "recharts": "^3.1.2",
    "xlsx": "^0.18.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^5.0.8"
  }
}
